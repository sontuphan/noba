{
  "name": "tare",
  "version": "1.0.0",
  "description": "The test framework for Bare",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --clean --format cjs,esm --dts --sourcemap",
    "pretest": "npm run build && tsup tests/**/*.ts --clean --silent --external tare --format cjs --out-dir tmp",
    "test": "node ./tmp/equal.test.js"
  },
  "keywords": [
    "test",
    "bare"
  ],
  "author": "Tu Phan <tu.phan@tether.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "bare": "^1.22.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
